module main;

    #The below code will get input of n numbers and print product

begin

    var n:int;
    n := input;

    var product:int;
    product := 1;

    var inp:int;

    for 0 to n
        do
            inp := input;
            product := product * inp;
        end;

    output := "Input of all numbers are: ";
    output := product;

end main.